{% extends ('base_admin.html.twig') %}

{% block body %}
<h2>Vos messages ! </h2>
{% for contact in contacts %}

<table>
    <thead>
        <tr>
            <th>Email</th>
            <th>Telephone</th>
            <th>Sujet</th>
            <th>Message</th>
        </tr>

    </thead>
    <tbody>
        <tr>
            <td>{{ contact.email }}</td>
            <td>{{ contact.telephone }}</td>
            <td>{{ contact.sujet }}</td>
            <td>{{ contact.message }}</td>
        </tr>
    </tbody>
    </table>
    {% endfor %}

{% endblock body %}
